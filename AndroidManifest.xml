<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.semantics3.pricematch"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.semantics3.pricematch.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.semantics3.pricematch.ListSitesActivity"
            android:label="@string/title_activity_list_sites" >
        </activity>
        <activity
            android:name="com.semantics3.pricematch.Listsites2Activity"
            android:label="@string/title_activity_listsites2"
            android:parentActivityName="com.semantics3.pricematch.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.semantics3.pricematch.MainActivity" />
        </activity>
    </application>

</manifest>
